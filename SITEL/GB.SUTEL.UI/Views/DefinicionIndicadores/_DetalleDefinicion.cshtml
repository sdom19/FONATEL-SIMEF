@using GB.SIMEF.Resources;
@model GB.SIMEF.Entities.DefinicionIndicador
<form>
    <div class="row">
        <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">
            <div class="form-group row">
                <label for="txtCodigoDefinicion" class="col-sm-12  col-form-label">@EtiquetasViewDefinicionIndicadores.LabelCodigo</label>
                <div class="col-sm-12 ">
                    @Html.EditorFor(model => model.Indicador.Codigo, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = EtiquetasViewCategorias.Nombre, id = "txtCodigo" } })

                </div>
            </div>

            <div class="form-group row">
                <label for="txtDireccionDefinicion" class="col-sm-12  col-form-label">@EtiquetasViewDefinicionIndicadores.LabelDirección</label>
                <div class="col-sm-12 ">
                    <input type="text" class="form-control form-control-fonatel disabled" id="txtDireccionDefinicion" value="Fonatel">
                </div>
            </div>
        </div>
        <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">

            <div class="form-group row">
                <label for="txtNombreDefinicion" class="col-sm-12  col-form-label">@EtiquetasViewDefinicionIndicadores.LabelNombreDefinicion</label>
                <div class="col-sm-12 ">
                    @Html.EditorFor(model => model.Indicador.Nombre, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = EtiquetasViewCategorias.Nombre, id = "txtIndicador" } })
                </div>
            </div>
            <div class="form-group row">
                <label for="txtServicioDefinicion" class="col-sm-12  col-form-label">@EtiquetasViewDefinicionIndicadores.LabelServicioDefinicion</label>
                <div class="col-sm-12 ">
                    @Html.EditorFor(model => model.Indicador.TipoIndicadores.Nombre, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = EtiquetasViewCategorias.Nombre, id = "txtServicioDefinicion" } })
                </div>
            </div>

        </div>

        <div class="row mt-5">
            <div class="col-sm-10 col-sm-offset-0 col-md-10 col-md-offset-1">
                <div class="form-group row">

                    <label for="txtDefinicion" class="col-sm-12 col-form-label">@EtiquetasViewDefinicionIndicadores.LabelDefinicion</label>
                    <div class="col-sm-12">

                        @Html.EditorFor(model => model.Definicion, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = @EtiquetasViewDefinicionIndicadores.LabelDefinicion, id = "txtDefinicion" } })
                    </div>
                </div>
            </div>
            <div class="col-sm-10 col-sm-offset-0 col-md-10 col-md-offset-1">
                <div class="form-group row">
                    <label for="txtFuenteDefinicion" class="col-sm-12 col-form-label">@EtiquetasViewDefinicionIndicadores.LabelFuenteDefinicion</label>
                    <div class="col-sm-12">
                        @Html.EditorFor(model => model.Fuente, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = @EtiquetasViewDefinicionIndicadores.LabelFuenteDefinicion, id = "txtFuenteDefinicion" } })
                    </div>
                </div>
            </div>
            <div class="col-sm-10 col-sm-offset-0 col-md-10 col-md-offset-1">
                <div class="form-group row">
                    <label for="txtNotasDefinicion" class="col-sm-12 col-form-label">@EtiquetasViewDefinicionIndicadores.LabelNotasDefinicion</label>
                    <div class="col-sm-12">
                        @Html.EditorFor(model => model.Notas, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", placeholder = @EtiquetasViewDefinicionIndicadores.LabelNotasDefinicion, id = "txtNotasDefinicion" } })
                    </div>
                </div>
            </div>
        </div>



        <div class="col-12 text-center mt-4">
            <button type="button" id="btnAtrasDefinicion" class="btn btn-fonatel btn-goback" aria-label="Left Align">
                <span aria-hidden="true"></span>@Etiquetas.BotonAtras
            </button>
        </div>
    </div>

</form>

