@using GB.SUTEL.UI.Helpers;

<h4>Mantenimiento de Servicios por Indicador</h4>
<hr>
@if (rolValidation.ValidateScreenFunctions(this.Context, "Servicio Indicador", "Crear"))
{
    <p>
        @Html.ActionLink("Nuevo", "Crear", "ServicioIndicador", null, new { @class = "btn btn-success" , title= GB.SUTEL.Resources.Resources.btnTooltipNuevo})
    </p>
}
<div id="tabla">
    @Html.Action("_table")
</div>

@if (rolValidation.ValidateScreenFunctions(this.Context, "Servicio Indicador", "Eliminar"))
{
    @Html.Partial("_eliminar");
}
@section scripts{
    @Scripts.Render("~/Scripts/Mantenimientos/source.js")
}