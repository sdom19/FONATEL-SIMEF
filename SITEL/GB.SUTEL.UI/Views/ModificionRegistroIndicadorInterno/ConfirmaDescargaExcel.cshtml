
<div class="modal fade" id="modalRegistroIndicadoresDescargar">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" tabindex="-1" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Descargar Archivo Excel</h4>
            </div>
            <div class="modal-body">

                <div class="form-horizontal">

                    <div class="form-group">

                        <div class="col-sm-11">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label for="txtOperadorNombreDescarga" class="col-sm-4 control-label">Operador</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="txtOperadorNombreDescarga" disabled="True">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtDescripcionServicio" class="col-sm-4 control-label">Descripción Servicio</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="txtDescripcionServicio" disabled="True">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtNombreAnno" class="col-sm-4 control-label">Año</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="txtano" placeholder="Año">
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="ddlEstado" class="col-sm-4 control-label">Estado</label>
                                    <div class="col-sm-8">
                                        <select class="form-control" id="ddlEstado">
                                            <option value="0" selected>Todos</option>
                                            <option value="1">Pendiente</option>
                                            <option value="2">Enviado</option>
                                            <option value="3">En Proceso</option>
                                            <option value="4">Finalizado</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="txtNombreFile" class="col-sm-4 control-label">Nombre del archivo</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="txtNombreFile" placeholder="Nombre" value="Plantilla">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
            <div class="modal-footer">
                <button type="button" tabindex="-1" class="btn btn-success" data-dismiss="modal" id="btnDescargarExcelRegistroIndicadores" onclick="descargarExcelRegistroIndicadores()">Descargar Excel</button>
                <button type="button" tabindex="-1" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

@section scripts{
    @Scripts.Render("~/Scripts/Proceso/ModificacionRegistroIndicador/jsModificarRegistroIndicadorInterno.js")

    <script>
        RequiredMessage = "@GB.SUTEL.Entities.Metadata.EntityErrors.Requerido";
    </script>
}