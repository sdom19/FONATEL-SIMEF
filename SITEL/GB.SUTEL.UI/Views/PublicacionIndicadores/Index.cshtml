@using GB.SIMEF.Resources;

@{
    ViewBag.Title = "Publicación de Indicadores";
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
    @Scripts.Render("~/bundles/FONATEL/PublicacionIndicador")
}

<div class="container body-content">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">@EtiquetasViewPublicaciones.PublicacionIndicadores</li>
            </ol>
        </nav>
    </div>
    <div class="row card">
        <div class="col-lg-12 mt-5">
            <h2 class="page-title-fonatel text-center">@EtiquetasViewPublicaciones.PublicacionIndicadores</h2>
            <hr />
        </div>
        <div class="col-md-12">
            <div class="col-lg-12 mt-5">
                @Html.Partial("_TablaIndicadores")
            </div>
        </div>

    </div>
</div>
@*<script src="~/Scripts/Fonatel/JsPublicar.js"></script>*@




