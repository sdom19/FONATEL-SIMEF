@using GB.SIMEF.Resources;

@{
    ViewBag.Title = EtiquetasViewCategorias.Categorias;
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

<script>
    const consultasFonatel = @ViewBag.ConsultasFonatel;
</script>

@*@section scripts {
    @Scripts.Render("~/bundles/FONATEL/CategoriaDesagregacion")
}*@

<div class="container body-content">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action( "index","Home")">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">@EtiquetasViewCategorias.Categorias</li>
            </ol>
        </nav>
    </div>
    <div id="FormularioIndex" class="row">
        <div class="col-lg-12">
            <h2 class="page-title-fonatel text-center">@EtiquetasViewCategorias.Categorias</h2>
            <hr />
        </div>
        <div class="col-lg-12 mt-4">
            <a class="btn btn-fonatel btn-success-fonatel" data-toggle="tooltip" data-placement="top" title="@Etiquetas.BotonCrear" alt="@Etiquetas.BotonCrear" href="@Url.Action( "create","CategoriasDesagregacion")">@Etiquetas.BotonCrear</a>
        </div>
        <div class="col-lg-12 mt-4">
            @Html.Partial("_TablaCategoriaDesagregacion")
            <input id='inputFileCargarDetalle' type='file' accept='.csv,.xlsx,.xls' style='display: none;' />
        </div>
    </div>

</div>

<script src="~/Scripts/Fonatel/JsCategoria.js"></script>

