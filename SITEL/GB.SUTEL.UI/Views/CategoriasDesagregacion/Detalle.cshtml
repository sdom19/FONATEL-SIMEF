@using GB.SIMEF.Resources;

@model GB.SIMEF.Entities.CategoriasDesagregacion


@{
    ViewBag.Title = Etiquetas.DetalleCategorias;
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
    @*@Scripts.Render("~/bundles/FONATEL/CategoriaDesagregacion")*@
}

<div class="container body-content">

    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item"><a href="/CategoriasDesagregacion">@Etiquetas.Categorias</a></li>
                <li class="breadcrumb-item active" aria-current="page">Detalle Categoría Desagregación</li>
            </ol>
        </nav>
    </div>


    <div id="FormularioDetalle" class="row">
        @Html.HiddenFor(model => model.id, new { id = "txtidCategoria" });
        <div class="col-lg-12">
            <h2 class="page-title-fonatel text-center">Detalle Categoría de Desagregación</h2>
            <hr />
        </div>
        <div class="col-lg-12">

            @Html.Partial("_CrearDetalle")
        </div>
        <div class="col-lg-12 mt-4">
            @Html.Partial("_TablaDetalleDesagregacion")
        </div>
    </div>

</div>
<script src="~/Scripts/Fonatel/JsCategoria.js"></script>
