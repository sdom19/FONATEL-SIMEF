@using GB.SIMEF.Resources;

@{
    ViewBag.Title = "Indicadores";
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
    @Scripts.Render("~/bundles/FONATEL/IndicadorFonatel")
}
<div class="container body-content">
    <input type="hidden" id="dad1f1ea">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action( "index","Home")">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">@EtiquetasViewIndicadorFonatel.TituloIndex</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12 mt-4">
            <h2 class="page-title-fonatel text-center">@EtiquetasViewIndicadorFonatel.TituloIndex</h2>
            <hr />
        </div>
        <div class="col-lg-12 mt-4">
            <a class="btn btn-fonatel btn-success-fonatel" data-toggle="tooltip" data-placement="top" title="@Etiquetas.BotonCrear" alt="@Etiquetas.BotonCrear" href="@Url.Action( "create","IndicadorFonatel")">@Etiquetas.BotonCrear</a>
        </div>
        <div class="col-md-12 mt-4">
            @Html.Partial("_TablaIndicador")
        </div>
    </div>
</div>