@model GB.SUTEL.UI.Models.SolicitudViewModels


<form id="formEliminar" method="post"
     
      
    data-ajax-message="Eliminando Solicitud Indicador...">
    @Html.Hidden("ItemEliminar", "", new { @id = "ItemEliminar" })

    <div class="modal fade" id="modalEliminarSolicitud">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                    <h4 class="modal-title">Eliminar Solicitud Indicador</h4>
                </div>
                <div class="modal-body">

                    <div class="form-horizontal">
                        @Html.ValidationSummary(true)
                        <div class="form-group">

                            <div class="col-md-10">
                                ¿Está seguro(a) que desea eliminar la solicitud seleccionada para un operador en específico?
                                <br />
                            </div>
                            <div>
                                <label for="txtOperadorNombreDescarga" class="col-sm-4 control-label">Operador</label>
                                <div class="col-sm-8">
                                    <select id="ddlOperadorDescarga" class="form-control" style="width: 100%"  multiple="multiple"></select>
                                </div>
                            </div>
                            <div class="checkbox col-sm-6">
                                <label>
                                    Eliminar Solicitud Completa
                                    <input type="checkbox" id="Completa" value="Completa">
                                </label>
                                </div>
                            
                        </div>
                        <div class="alert alert-danger hidden" role="alert" id="divMensajeErrorEliminarSolicitud">
                            <span id="errorMensajeEliminar"></span>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <input type="submit" id="mySubmit" value="Eliminar" class="btn btn-success" data-tooltip="true" title="Confirmar eliminación" />
                        <button type="button" class="btn btn-danger" data-dismiss="modal" data-tooltip="true" title="Cancelar acción de eliminación">Cancelar</button>
                    </div>
                </div>

            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
</form>

<script>

   

   
</script>

