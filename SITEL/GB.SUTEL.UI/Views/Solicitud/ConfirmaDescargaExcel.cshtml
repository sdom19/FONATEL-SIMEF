
    <div class="modal fade" id="modalRegistroIndicadoresDescargarPre">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Previsualizar Archivo Excel</h4>
            </div>
            <div class="modal-body">

                <div class="form-horizontal">

                    <div class="form-group">

                        <div class="col-sm-11">
                            <input type="hidden" id="IDSolicitudIndicador" value="">
                            </br>

                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label for="txtNombreFile" class="col-sm-4 control-label">Nombre del archivo</label>
                                    <div class="col-sm-8">
                                        <input type="text" class="form-control" id="txtNombreFile" placeholder="Nombre" value="Plantilla">
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label for="txtNombreFile" class="col-sm-4 control-label">Tipo de Formato</label>
                                    <div class="col-sm-8">
                                        <div class="radio">
                                            <label>
                                                <input type="radio" name="optionsRadiosExtensionExcel" id="ExtensionExcelXLSX" value="xlsm" checked>
                                                .xlsm (Excel 2007 - 2013)
                                            </label>
                                        </div>
                                        @if (@System.Configuration.ConfigurationManager.AppSettings["enableOffice2003"].ToUpper().ToString() == "TRUE")
                                        {
                                          <div class="radio">
                                            <label>
                                                <input type="radio" name="optionsRadiosExtensionExcel" id="ExtensionExcelXLS" value="xls">
                                                .xls (Excel 97 - 2003)
                                            </label>
                                            </div>   
                                        }                                       
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>

            </div>
            <div class="modal-footer">                
                <button type="button" class="btn btn-success" data-dismiss="modal" id="btnDescargarExcelRegistroIndicadores" onclick="descargarExcelRegistroIndicadores()">Previsualizar Excel</button>
                <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->

<script type="text/javascript">

    function descargarExcelRegistroIndicadores() {      
        var IdSolicitud = $("#IDSolicitudIndicador").val();
        var registroIndicadoresNombreExcel = $("#txtNombreFile").val();
        var registroIndicadoresExtension = $('input[name=optionsRadiosExtensionExcel]:checked').val();

        if (IdSolicitud == "" || IdSolicitud == null) {
            return false;
        }

        if (registroIndicadoresNombreExcel == "" || registroIndicadoresNombreExcel == null) {
            registroIndicadoresNombreExcel = "previsualizacion";
        }

        if (registroIndicadoresExtension == "" || registroIndicadoresExtension == null) {
            registroIndicadoresNombreExcel = "xlsx";
        }        
        //debugger;
        window.open("/Solicitud/DescargarExcel?IdSolicitud=" + IdSolicitud + "&registroIndicadoresNombreExcel=" + registroIndicadoresNombreExcel + "&registroIndicadoresExtension=" + registroIndicadoresExtension, '_blank');
    }
</script>