@using GB.SIMEF.Resources;

@{
    ViewBag.Title = "Generador URL de Indicadores";
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
    @Scripts.Render("~/bundles/FONATEL/GeneradorURL")
}

<div class="container body-content">
    <input type="hidden" id="dad1f1ea">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="@Url.Action( "index","Home")">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">@EtiquetasViewIndicadorFonatel.TituloGeneradorUrl</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12 mt-4">
            <h2 class="page-title-fonatel text-center">@EtiquetasViewIndicadorFonatel.TituloGeneradorUrl</h2>
            <hr />
        </div>
        <div class="col-lg-12 mt-4">
            <form class="form-inline col-lg-12" role="form">
                <div class="form-group col-lg-2" style="padding-left: 0px; margin-left: 0px;">
                    <a class="btn btn-fonatel btn-success-fonatel" data-toggle="tooltip" data-placement="top" title="@Etiquetas.Generar" alt="@Etiquetas.Generar"  style="margin-left: 0px;" id="btnGenerarURL">@Etiquetas.Generar</a>
                </div>
                <div class="form-group col-lg-10 mt-2" style="padding-right: 0px; margin-right: 0px;">
                    <input class="form-control form-control-fonatel solo_texto text-box single-line" style="max-width: 100%;" type="text" placeholder="URL" name="URL" id="txtURL" disabled value="@ViewBag.RutaAPISIGITEL"/>
                    <input type="hidden" id="txtRuta" value="@ViewBag.RutaAPISIGITEL"/>
                </div>
            </form>
        </div>

    </div>
    <div class="col-md-12 mt-4" style="margin-left: 14px; padding-right: 40px;">
        @Html.Partial("_Tabla")
    </div>
</div>

