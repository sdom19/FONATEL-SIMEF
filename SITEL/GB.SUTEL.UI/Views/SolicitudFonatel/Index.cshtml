@using GB.SIMEF.Resources;

@{
    ViewBag.Title = "Solicitudes";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container body-content">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active" aria-current="page">Solicitudes de Información</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <h2 class="page-title-fonatel text-center">Solicitudes de Información</h2>
            <hr />
        </div>
        <div class="col-lg-12 mt-4">
            <a class="btn btn-fonatel btn-success-fonatel" data-toggle="tooltip" data-placement="top" title="@Etiquetas.BotonCrear" alt="@Etiquetas.BotonCrear" href="/Fonatel/SolicitudFonatel/Create">@Etiquetas.BotonCrear</a>
        </div>

        <div class="col-lg-12 mt-4 ">
            @Html.Partial("_TablaSolicitud")
        </div>
    </div>

    @Html.Partial("_ModalSolicitud")

</div>


