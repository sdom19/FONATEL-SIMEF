@model GB.SUTEL.UI.Models.DetalleAgrupacionViewModels


    <form id="formEliminar" method="post"
          action="@Url.Action("Eliminar")" data-ajax="true"
          data-ajax-message="Eliminando Agrupación...">
        @Html.Hidden("ItemEliminarIdDetalleAgrupacion", "", new { @id = "ItemEliminarIdDetalleAgrupacion" })
        @Html.Hidden("ItemEliminarOperador", "", new { @id = "ItemEliminarOperador" })
        @Html.Hidden("ItemEliminarAgrupacion", "", new { @id = "ItemEliminarAgrupacion" })
        <div class="modal fade" id="modalEliminarDetalle">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">Eliminar Detalle Agrupación</h4>
                    </div>
                    <div class="modal-body">

                        <div class="form-horizontal">
                            @Html.ValidationSummary(true)
                            <div class="form-group">
                               
                                <div class="col-md-10">
                               
                                    ¿Está seguro que desea eliminar el Detalle Agrupación Seleccionado?
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <input type="submit" id="mySubmit" value="Eliminar" class="btn btn-success" alt="Eliminar" title="Eliminar" />
                        <button type="button" class="btn btn-danger" data-dismiss="modal" title="Cancelar">Cancelar</button>
                        
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->

    </form>

