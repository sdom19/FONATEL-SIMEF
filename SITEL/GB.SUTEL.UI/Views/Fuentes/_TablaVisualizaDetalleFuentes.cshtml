@model GB.SIMEF.Entities.FuenteRegistro
@using GB.SIMEF.Resources;
<div class="table-responsive table-wrapper-fonatel">

    <table id="TableDetalleFuentes" class="dataTable datatable_simef table-bordered table-condensed table-hover">
        <thead>
            <tr>
                <th scope="col">@EtiquetasViewFuentesRegistro.Nombre</th>
                <th scope="col">@EtiquetasViewFuentesRegistro.Correo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model.DetalleFuenteRegistro)
            {
                <tr>
                    <th scope="row">@item.NombreDestinatario</th>
                    <td>@item.CorreoElectronico</td>
                </tr>
            }
        </tbody>

    </table>

</div>

