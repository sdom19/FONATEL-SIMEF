@using GB.SIMEF.Resources;
@model GB.SIMEF.Entities.RelacionCategoria




<div class="row">
    <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">


        <div class="form-group row">
            <label for="txtNombre" class="col-sm-12 col-form-label">@EtiquetasViewRelacionCategoria.NombreRelacion</label>
            <div class="col-sm-12">
                @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", id = "txtNombreRelacion" } })
            </div>
        </div>
        <div class="form-group row">
            <label for="txtCodigo" class="col-sm-12 col-form-label">Código</label>
            <div class="col-lg-12">
                @Html.EditorFor(model => model.Codigo, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", id = "txtCodigoRelacion" } })
            </div>
        </div>

        <div class="form-group row">
            <label for="txtCantidad" class="col-sm-12 col-form-label">@EtiquetasViewRelacionCategoria.Cantidad</label>
            <div class="col-sm-12">
                @Html.EditorFor(model => model.CantidadCategoria, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", id = "TxtCantidadCategoria" } })
            </div>
        </div>
    </div>
    <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">

        <div class="form-group row">
            <label for="ddlCategoriaId" class="col-sm-12 col-form-label ">Categoría ID</label>
            <div class="col-sm-12">
                @Html.EditorFor(model => model.CategoriasDesagregacionid.NombreCategoria, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", id = "txtcategoriaid" } })
            </div>
        </div>

        <div class="form-group row">
            <label for="txtCantidadRegistros" class="col-sm-12 col-form-label">@EtiquetasViewRelacionCategoria.CantidadRegistros</label>
            <div class="col-sm-12">
                @Html.EditorFor(model => model.CantidadFila, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled", id = "txtCantidadFilas" } })
            </div>
        </div>
    </div>

</div>
<div class="row">
    <div class="col-lg-12 text-center">
        <button type="button" id="btnSiguienteRelacionCategoriaView"  class="btn btn-fonatel btn-next-fonatel" data-toggle="tooltip" data-placement="top" title="@Etiquetas.BotonSiguiente">@Etiquetas.BotonSiguiente</button>
        <button type="submit" id="btnCancelar" data-toggle="tooltip" data-placement="top" title="@Etiquetas.BotonCancelar" class="btn btn-fonatel btn-error-fonatel">@Etiquetas.BotonCancelar</button>
    </div>
</div>



