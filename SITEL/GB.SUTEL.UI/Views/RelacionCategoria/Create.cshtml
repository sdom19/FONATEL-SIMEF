@using GB.SIMEF.Resources;

@{
    ViewBag.Title = ViewBag.titulo;
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
  @*  @Scripts.Render("~/bundles/FONATEL/RelacionCategoria")*@
}

<div class="container body-content">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active"><a href="/Fonatel/RelacionCategoria/index">@EtiquetasViewRelacionCategoria.RelacionCategoria</a></li>
                <li class="breadcrumb-item active" aria-current="page">@ViewBag.titulo</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div>
            <h2 class="page-title-fonatel text-center">@ViewBag.titulo</h2>
            <hr />
        </div>
        <div class="col-lg-12">
            <form id="FormularioCrearRelacion">
                @Html.Partial("_CrearRelacionCategoria")
            </form>
        </div>
    </div>

</div>

<script src="~/Scripts/Fonatel/JsRelacionCategoria.js"></script>