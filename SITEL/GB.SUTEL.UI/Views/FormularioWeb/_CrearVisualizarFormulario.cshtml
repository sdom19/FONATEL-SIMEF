@using GB.SIMEF.Resources;

@model GB.SIMEF.Entities.FormularioWeb

<div>
    <div class="row">
        <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">
            <div class="justify-content-center">
                <div class="form-group row">
                    <label class="col-sm-12 col-form-label">@EtiquetasViewFormulario.Codigo</label>
                    <div class="col-sm-12">
                        @Html.EditorFor(model => model.Codigo, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled" } })
                    </div>
                </div>

                <div class="form-group row">
                    <label class="col-sm-12 col-form-label">@EtiquetasViewFormulario.Nombre</label>
                    <div class="col-sm-12">
                        @Html.EditorFor(model => model.Nombre, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled" } })
                    </div>
                </div>

            </div>
        </div>
        <div class="col-md-5 col-sm-12 col-md-offset-1 col-sm-offset-0">
            <div class="justify-content-center">
                <div class="form-group row">
                    <label class="col-sm-12 col-form-label ">@EtiquetasViewFormulario.FrecuenciaEnvio</label>
                    <div class="col-sm-12">
                        <div class="select2-wrapper">
                            <div class="select2-wrapper">
                                @Html.EditorFor(model => model.FrecuenciaEnvio.Nombre, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled" } })
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group row">
                    <label class="col-sm-12 col-form-label">@EtiquetasViewFormulario.CantidadIndicadores</label>
                    <div class="col-sm-12">
                        @Html.EditorFor(model => model.CantidadIndicador, new { htmlAttributes = new { @class = "form-control form-control-fonatel disabled" } })
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-11 col-md-offset-1 col-sm-12 col-sm-offset-0">
            <div class="form-group row">
                <label class="col-sm-12 col-form-label">@EtiquetasViewFormulario.Descripcion</label>
                <div class="col-sm-12">
                    @Html.TextAreaFor(model => model.Descripcion, new { @class = "form-control form-control-fonatel disabled" })
                </div>
            </div>
        </div>
    </div>
</div>