@model GB.SUTEL.UI.Models.SolicitudViewModels


    <form id="formNotificar" method="post"
          action="@Url.Action("Notificar")" data-ajax="true"
          data-ajax-message="Notificando Solicitud Indicador...">

        @Html.Hidden("ItemNotificar", "", new { @id = "ItemNotificar" })
        <div class="modal fade" id="modalNotificarSolicitud">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title">Notificar Solicitud Indicador</h4>
                    </div>
                    <div class="modal-body">

                        <div class="form-horizontal">
                            @Html.ValidationSummary(true)
                            <div class="form-group">
                               
                                <div class="col-md-10">
                               
                                    ¿Está seguro que desea notificar la Solicitud Seleccionado?
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="modal-footer">
                        <input type="submit" id="mySubmit" value="Sí" class="btn btn-success" alt="Sí" title="Sí" />
                        <button type="button" class="btn btn-danger" data-dismiss="modal" title="No">No</button>
                       
                    </div>
                </div><!-- /.modal-content -->
            </div><!-- /.modal-dialog -->
        </div><!-- /.modal -->

    </form>