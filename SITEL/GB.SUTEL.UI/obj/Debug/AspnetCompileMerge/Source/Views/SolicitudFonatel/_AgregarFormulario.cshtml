@using GB.SIMEF.Resources;

<form>
    <div class="row">
        <div class="col-md-12 col-sm-12">

            <div class="form-group center">
                <label for="ddlVariableIndicador" class="col-form-label">@EtiquetasViewSolicitudes.Formulario</label>
                <div class="select2-wrapper">
                    @Html.DropDownList("ddlFormularioWeb", new SelectList(ViewBag.ListaFormularioWeb, "Value", "Text"), "Seleccione", new { @class = "listasDesplegables", id = "ddlFormularioWeb" })       
                </div>
                <small id="ddlVariableIndicadorHelp" class="form-text form-text-danger-fonatel hidden">@Etiquetas.CampoRequerido</small>
            </div>
        </div>
    </div>
    <div class="row mt-5">
        <div class="col-lg-12 text-center">
            <button type="submit" id="btnGuardarFormulario" class="btn btn-fonatel btn-success-fonatel">@Etiquetas.BotonGuardar</button>
            <button id="btnCancelarFormulario" type="submit" class="btn btn-fonatel btn-error-fonatel">@Etiquetas.BotonCancelar</button>
        </div>
    </div>
</form>
