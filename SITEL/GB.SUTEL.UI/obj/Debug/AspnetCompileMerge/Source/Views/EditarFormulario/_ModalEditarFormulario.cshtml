
@using GB.SIMEF.Resources;

<div class="modal fade modal-fonatel" data-backdrop="static" id="modalEditarFormulario" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
<div class="modal-dialog modal-dialog-centered modal-fonatel" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
            <h5 class="modal-title" id="exampleModalLongTitle">Editar Formulario</h5>
        </div>
        <div class="modal-body">
            <div class="row">
                <div class="col-sm-12 ">
                    <div class="form-group center">
                        <label for="ddlFuenteDescargaEditarFormulario" class="col-sm-12 col-form-label">Fuente</label>
                        <div class="col-sm-12">
                            <div class="select2-wrapper">
                                <select class="listasDesplegables" id="ddlFuenteEditarFormulario">
                                    <option></option>
                                    <option value="1">FUENTE 1</option>
                                    <option value="2">FUENTE 2</option>
                                    <option value="2">FUENTE 3</option>
                                </select>
                            </div>
                            <small id="txtNombreFormularioHelp" class="form-text form-text-danger-fonatel">@Etiquetas.CampoRequerido</small>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group center">
                        <label for="ddlSolicitudEditarFormulario" class="col-sm-12 col-form-label">Solicitud</label>
                        <div class="col-sm-12">
                            <div class="select2-wrapper">
                                <select class="listasDesplegables" id="ddlSolicitudEditarFormulario">
                                    <option></option>
                                    <option value="1">SOLICITUD 1</option>
                                    <option value="2">SOLICITUD 2</option>
                                    <option value="2">SOLICITUD 3</option>
                                </select>
                            </div>
                            <small id="txtNombreFormularioHelp" class="form-text form-text-danger-fonatel">@Etiquetas.CampoRequerido</small>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group center">
                        <label for="ddlFormularioEditarFormulario" class="col-sm-12 col-form-label">Formulario</label>
                        <div class="col-sm-12">
                            <div class="select2-wrapper">
                                <select class="listasDesplegables" id="ddlFormularioEditarFormulario">
                                    <option></option>
                                    <option value="1">SOLICITUD 1</option>
                                    <option value="2">SOLICITUD 2</option>
                                    <option value="2">SOLICITUD 3</option>
                                </select>
                            </div>
                            <small id="txtNombreFormularioHelp" class="form-text form-text-danger-fonatel">@Etiquetas.CampoRequerido</small>
                        </div>
                    </div>


                    <div class="form-group center">
                        <label for="ddlMesEditarFormulario" class="col-sm-12 col-form-label">Mes</label>
                        <div class="col-sm-12">
                            <div class="select2-wrapper">
                                <select class="listasDesplegables" id="ddlMesEditarFormulario">
                                    <option></option>
                                    <option value="1">ENERO</option>
                                    <option value="2">FEBRERO</option>
                                    <option value="2">MARZO</option>
                                </select>
                            </div>
                            <small id="txtNombreFormularioHelp" class="form-text form-text-danger-fonatel">@Etiquetas.CampoRequerido</small>
                        </div>
                    </div>


                    <div class="form-group center">
                        <label for="ddlAnnoEditarFormulario" class="col-sm-12 col-form-label">Año</label>
                        <div class="col-sm-12">
                            <div class="select2-wrapper">
                                <select class="listasDesplegables" id="ddlAnnoEditarFormulario">
                                    <option></option>
                                    <option value="1">2019</option>
                                    <option value="2">2020</option>
                                    <option value="2">2021</option>
                                </select>
                            </div>
                            <small id="txtNombreFormularioHelp" class="form-text form-text-danger-fonatel">@Etiquetas.CampoRequerido</small>
                        </div>
                    </div>



                </div>


            </div>

        </div>
        <div class="modal-footer">
            <div class="text-center">
                 <button type="submit" id="btnGuardarEditarFormulario" class="btn btn-fonatel btn-success-fonatel">@Etiquetas.BotonGuardar</button>
                 <button type="submit" id="btnCancelaEditarFormulario" class="btn btn-fonatel btn-error-fonatel">@Etiquetas.BotonCancelar</button>
             </div>        
        </div>
     </div>
 </div>
</div>








