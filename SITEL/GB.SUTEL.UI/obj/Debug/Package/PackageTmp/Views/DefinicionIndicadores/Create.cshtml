@using GB.SIMEF.Resources;

@{
    ViewBag.Title = ViewBag.Titulo;
    Layout = "~/Views/Shared/_LayoutFONATEL.cshtml";
}

@section scripts {
    @Scripts.Render("~/bundles/FONATEL/DefinicionIndicador")
}

<div class="container body-content">
    <div class="text-right">
        <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/">@Etiquetas.LabelInicio</a></li>
                <li class="breadcrumb-item active"><a href="/Fonatel/DefinicionIndicadores/index">@EtiquetasViewIndicadorFonatel.TituloDefiniciones</a></li>
                <li class="breadcrumb-item active" aria-current="page">@ViewBag.Titulo</li>
            </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <h2 class="page-title-fonatel text-center">@ViewBag.Titulo</h2>
            <hr />
        </div>
        <div class="col-lg-12">

            @Html.Partial("_CreateDefinicion")
        </div>
    </div>

</div>
