<div class="modal fade" id="modalRegistroIndicadoresImportar">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" tabindex="-1" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Carga de Archivo Excel</h4>
            </div>
            <div class="modal-body">

                <div class="form-horizontal">

                    <div class="form-group">

                        <div class="col-sm-11">
                            @if (ViewBag.Error != null)
                            {
                                <div class="alert alert-danger">
                                    @ViewBag.Error
                                </div>
                            }

                        <form id="formularioCarga" enctype="multipart/form-data">
                            @Html.AntiForgeryToken()
                            <div>

                                @GB.SUTEL.Resources.Resources.RegistroIndicador_msjUsuarioCargaExel

                            </div>
                            <div class="form-group">
                                @Html.Label("Seleccionar archivo excel", new { @class = "control-label col-md-5" })
                                <div class="col-md-7">
                                    <input type="file" id="fileImportar" name="fileImportar" class="" />
                                </div>
                            </div>

                            <div class="modal-footer">

                                <input type="submit" tabindex="-1" value="@GB.SUTEL.Resources.Resources.btnCargaArchivo" class="btn btn-success" title="@GB.SUTEL.Resources.Resources.btnCargaArchivo" alt="@GB.SUTEL.Resources.Resources.btnCargaArchivo">
                                <button type="button" tabindex="-1" class="btn btn-danger" data-dismiss="modal" title="Salir">Cancelar</button>

                            </div>
                            <br />
                            <div id="mensaje"></div>
                        </form>
                        </div>
                    </div>
                </div>
            </div>
                        </div><!-- /.modal-content -->
                    </div><!-- /.modal-dialog -->
                </div><!-- /.modal -->








@section scripts{
    @Scripts.Render("~/Scripts/Proceso/ModificacionRegistroIndicador/jsModificarRegistroIndicadorInterno.js")

    <script>
        RequiredMessage = "@GB.SUTEL.Entities.Metadata.EntityErrors.Requerido";
    </script>
}